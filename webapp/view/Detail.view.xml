				<ObjectHeader id="oh1" responsive="true" binding="" intro="(Nickname)" title="Name" number="0.0" numberUnit="Man-day" numberState="None"
					backgroundDesign="Translucent">
					<attributes>
						<ObjectAttribute title="Customer" text=""/>
						<ObjectAttribute title="Period" text=""/>
					</attributes>
					<!--<statuses>-->
					<!--	<ObjectStatus title="Approval" text="Pending"/>-->
					<!--</statuses>-->
					<headerContainer>
						<IconTabBar id="idIconTabBarSeparatorNoIcon" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
							<items>
								<IconTabFilter icon="sap-icon://hint" iconColor="Positive" text="Summary">
									<List id="timeSheetList" mode="{device>/listMode}" showSeparators="None">
										<items>
											<ObjectListItem title="Charge" type="Active" icon="sap-icon://lead" press="onItemPress" number="{view>/MDCharge}" numberUnit="Man-Day"></ObjectListItem>
											<ObjectListItem title="Non-Charge" type="Active" press="onItemPress" icon="sap-icon://message-warning" number="{view>/MDNon}"
												numberUnit="Man-Day"></ObjectListItem>
											<ObjectListItem title="Leave" type="Active" press="onItemPress" icon="sap-icon://accidental-leave" number="{view>/MDLeave}"
												numberUnit="Man-Day"></ObjectListItem>
										</items>
									</List>
								</IconTabFilter>
								<IconTabSeparator icon=""/>
								<IconTabFilter icon="sap-icon://survey" iconColor="Default" count="32">
									<content>
										<Table id="a" width="auto" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20"
											growingScrollToLoad="true" class="sapUiResponsiveMargin" items="">
											<columns>
												<Column width="8%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Date" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="30%" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Description" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="15%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Session" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="8%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Activity" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="8%" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Charge Type" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="8%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Man-Day" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="5%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Overtime" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
												<Column width="20%" hAlign="Left" vAlign="Top" minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
													<header>
														<Text text="Per-diem" width="39.703125px" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</header>
													<footer/>
												</Column>
											</columns>
											<items>
												<ColumnListItem type="Navigation" press="_onRowPress">
													<cells>
														<Link text="{Orderno}" textAlign="Begin" width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false"
															subtle="false" press="_onLinkPress"/>
														<Text text="{OrdDescription}" width="auto" maxLines="2" wrapping="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
														<Link href="{Location/LocationURL}" text="{Location/LocationDescription}" textAlign="Begin" width="auto" target="_blank" wrapping="false"
															enabled="true" visible="true" emphasized="false" subtle="false"/>
														<Text text="{Location/Ward}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
														<Text text="{MainWorkCenter}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
														<Text text="{Priority}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
														<Link text="{ReportedBy}" textAlign="Begin" width="auto" target="_blank" wrapping="false" enabled="true" visible="true" emphasized="false"
															subtle="false" press="_onLinkPress1"/>
														<Text text="{OrdStatus}" width="auto" maxLines="2" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
													</cells>
												</ColumnListItem>
											</items>
										</Table>
									</content>
								</IconTabFilter>
								<IconTabSeparator icon=""/>
								<IconTabFilter icon="sap-icon://appointment" iconColor="Default" count="0">
									<Text text="People content goes here ..."/>
								</IconTabFilter>
							</items>
						</IconTabBar>
					</headerContainer>
